<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sergio.betancourt.tarealogin.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="sergio.betancourt.tarealogin.app.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity
            android:name=".Principal"
            android:label="@string/title_activity_main" >
        </activity>

        <activity
            android:name="sergio.betancourt.tarealogin.app.Calculadora"
            android:label="@string/title_activity_calculadora" >
        </activity>
        <activity
            android:name="sergio.betancourt.tarealogin.app.Conversor"
            android:label="@string/title_activity_conversor" >
        </activity>

    </application>

</manifest>
